name: "ex3"

mesh: { path: "channel.msh" }
output: { formats: [VTK], directory: "../.out" }
physics: [IncFlow]
fluid: { viscosity: 2e-2, density: 1 }

velocity: {
    inflow_profile: [0.5, 0]
    initial_conditions: [0, 0]
}

boundaries: {
    inflow: ["inflow"],
    outflow: ["outflow"],
    wall: ["wall"]
}


time: {
    timestep: 0.05
    steps:  100
    output_frequency: 1
}

